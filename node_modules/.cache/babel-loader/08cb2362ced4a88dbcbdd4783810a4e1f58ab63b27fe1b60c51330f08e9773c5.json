{"ast":null,"code":"import onerooms from './assets/onerooms';\nimport discount from './components/DiscountProduct.vue';\nimport productCard from './components/ProductCard.vue';\nimport openModal from './components/OpenModal.vue';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      oneroomsOrigin: [...onerooms],\n      onerooms: onerooms,\n      menus: ['Home', 'Shop', 'About'],\n      report: [0, 0, 0],\n      viewModal: false,\n      clickProduct: 0,\n      discountOpacity: 1\n    };\n  },\n  methods: {\n    increase(i) {\n      this.report[i] += 1;\n    },\n    priceSort() {\n      this.onerooms.sort(function (a, b) {\n        return a.price - b.price;\n      });\n    },\n    priceSort2() {\n      this.onerooms.sort(function (a, b) {\n        return b.price - a.price;\n      });\n    },\n    sortBack() {\n      this.onerooms = [...this.oneroomsOrigin];\n    }\n  },\n  mounted() {\n    setTimeout;\n    this.discountOpacity;\n  },\n  components: {\n    discount,\n    openModal,\n    productCard\n  }\n};","map":{"version":3,"names":["onerooms","discount","productCard","openModal","name","data","oneroomsOrigin","menus","report","viewModal","clickProduct","discountOpacity","methods","increase","i","priceSort","sort","a","b","price","priceSort2","sortBack","mounted","setTimeout","components"],"sources":["C:\\Users\\qwert\\Documents\\vueStudy\\vuedongsan\\src\\App.vue"],"sourcesContent":["<template>\n\n<transition name=\"fade\">\n    <openModal\n    v-if=\"viewModal==true\"\n    :oneroom=\"onerooms[clickProduct]\"\n    @closeModal=\"viewModal=false\"/>\n  </transition>\n\n  <div class=\"menu\">\n    <a href=\"\" v-for=\"(menu,i) in menus\" :key=\"i\">{{i+1}} {{menu}}</a>\n  </div>    \n\n  <discount/>\n\n  <div class=\"sortButton\">\n    <button @click=\"priceSort2()\">가격순내림차순</button>\n    <button @click=\"priceSort()\">가격순오름차순</button>\n    <button @click=\"sortBack()\">정렬되돌리기</button>\n  </div>\n\n  <productCard \n    v-for=\"(oneroom,i) in onerooms\" :key=\"i\" \n    :oneroom=\"oneroom\" \n    @openModal=\"viewModal=true; clickProduct=i;\"/>\n  \n</template>\n\n<script>\n\nimport onerooms from './assets/onerooms';\nimport discount from './components/DiscountProduct.vue'\nimport productCard from './components/ProductCard.vue';\nimport openModal from './components/OpenModal.vue'\n\n\nexport default {\n  name: 'App',\n  data(){\n    return{\n      oneroomsOrigin : [...onerooms],\n      onerooms : onerooms,\n      menus : ['Home', 'Shop','About'],\n      report : [ 0,0,0 ],\n      viewModal : false,\n      clickProduct : 0,\n      discountOpacity : 1,\n    }\n  },\n\n  methods :  {\n    increase(i) {\n      this.report[i]+=1\n    },\n    priceSort(){\n      this.onerooms.sort(function(a,b){\n        return a.price-b.price\n      });\n    },\n    priceSort2(){\n      this.onerooms.sort(function(a,b){\n        return b.price-a.price\n      });\n    },\n    sortBack(){\n      this.onerooms = [...this.oneroomsOrigin];\n    },\n  },\n\n  mounted(){\n    setTimeout\n    this.discountOpacity\n  },\n\n  components: {\n    discount,\n    openModal,\n    productCard,\n  }\n}\n</script>\n\n<style>\n*{\n  margin: 0;\n}\n.menu{\n  background: darkslateblue;\n  padding: 15px;\n  border-radius: 5px;\n  text-align: center;\n}\n.menu a{\n  color: white;\n  padding: 10px;\n  }\n.discount{\n color: #000;\n}\n.product{\n  border: black 2px solid;\n  padding: 1rem;\n  margin: 1rem;\n  width: 900px;\n  margin-left: auto;\n  margin-right: auto;\n  text-align: center;\n}\n.product img{\n  width: 600px;\n}\n.black-bg{\n  width: 100%; height: 100%;\n  background: rgba(0,0,0,0.5);\n  position: fixed; padding: 20px;\n}\n.white-bg{\n  width: 50%; background-color: white;\n  border-radius: 8px;\n  padding: 20px;\n  margin: auto;\n  text-align: center;\n}\n.fade-enter-from{\nopacity: 0;\n}\n.fade-enter-active{\ntransition: all 0.5s;\n}\n.fade-enter-to{\nopacity: 1;\n}\n.fade-leave-from{\nopacity: 1;\n}\n.fade-leave-active{\ntransition: all 0.5s;\n}\n.fade-leave-to{\nopacity: 0;\n}\n.sortButton{\n  text-align: center;\n}\n\n\n</style>\n"],"mappings":"AA8BA,OAAOA,QAAO,MAAO,mBAAmB;AACxC,OAAOC,QAAO,MAAO,kCAAiC;AACtD,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,SAAQ,MAAO,4BAA2B;AAGjD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,cAAa,EAAI,CAAC,GAAGN,QAAQ,CAAC;MAC9BA,QAAO,EAAIA,QAAQ;MACnBO,KAAI,EAAI,CAAC,MAAM,EAAE,MAAM,EAAC,OAAO,CAAC;MAChCC,MAAK,EAAI,CAAE,CAAC,EAAC,CAAC,EAAC,EAAG;MAClBC,SAAQ,EAAI,KAAK;MACjBC,YAAW,EAAI,CAAC;MAChBC,eAAc,EAAI;IACpB;EACF,CAAC;EAEDC,OAAM,EAAK;IACTC,QAAQA,CAACC,CAAC,EAAE;MACV,IAAI,CAACN,MAAM,CAACM,CAAC,CAAC,IAAE;IAClB,CAAC;IACDC,SAASA,CAAA,EAAE;MACT,IAAI,CAACf,QAAQ,CAACgB,IAAI,CAAC,UAASC,CAAC,EAACC,CAAC,EAAC;QAC9B,OAAOD,CAAC,CAACE,KAAK,GAACD,CAAC,CAACC,KAAI;MACvB,CAAC,CAAC;IACJ,CAAC;IACDC,UAAUA,CAAA,EAAE;MACV,IAAI,CAACpB,QAAQ,CAACgB,IAAI,CAAC,UAASC,CAAC,EAACC,CAAC,EAAC;QAC9B,OAAOA,CAAC,CAACC,KAAK,GAACF,CAAC,CAACE,KAAI;MACvB,CAAC,CAAC;IACJ,CAAC;IACDE,QAAQA,CAAA,EAAE;MACR,IAAI,CAACrB,QAAO,GAAI,CAAC,GAAG,IAAI,CAACM,cAAc,CAAC;IAC1C;EACF,CAAC;EAEDgB,OAAOA,CAAA,EAAE;IACPC,UAAS;IACT,IAAI,CAACZ,eAAc;EACrB,CAAC;EAEDa,UAAU,EAAE;IACVvB,QAAQ;IACRE,SAAS;IACTD;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}